{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/**"
        ]
      }
    }
  ],
  "scripts": {
    "build": "prisma generate && next build"
  },
    "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "DELETE",
        "PUT"
      ]
    }
  ]
}